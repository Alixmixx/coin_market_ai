             <div _ngcontent-c9="" class="operation-samples">
              <!-- -->
              <endpoint-link _ngcontent-c9="" _nghost-c10="">
               <div _ngcontent-c10="" class="operation-endpoint">
                <h5 _ngcontent-c10="" class="http-verb get">
                 get
                </h5>
                <span _ngcontent-c10="">
                 <span _ngcontent-c10="" class="operation-api-url-path">
                  /v1/cryptocurrency/quotes/latest
                 </span>
                </span>
                <svg _ngcontent-c10="" class="expand-icon" version="1.1" viewbox="0 0 24 24" x="0" xml:space="preserve" xmlns="http://www.w3.org/2000/svg" y="0">
